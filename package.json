{
  "name": "form-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connected-react-router": "^4.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "forcedomain": "^1.0.0",
    "history": "^4.7.2",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "node-sass-chokidar": "^1.3.4",
    "query-string": "^6.2.0",
    "rc-slider": "^8.6.3",
    "react": "^16.5.2",
    "react-before-after-slider": "^1.0.4",
    "react-dom": "^16.5.2",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "react-sidebar": "^3.0.2",
    "react-stack-grid": "^0.7.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js postcss-watch",
    "build-js": "GENERATE_SOURCEMAP=false react-scripts build",
    "build": "npm-run-all build-css build-js postcss-build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "postcss-watch": "postcss src/styles/index.css -o src/styles/index.css -w",
    "postcss-build": "postcss src/styles/index.css -o src/styles/index.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "cross-env NODE_ENV=production node ./server/index.js",
    "prettier-run": "prettier --write 'src/**/*.js'",
    "heroku-postbuild": "npm run build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "husky": "^1.1.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.15.2"
  }
}
